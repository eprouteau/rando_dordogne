{
  "js": ["js/filter.js"],
  "css": "css/filter.css",
  "html": "filter.html",
  "target": "page-content-wrapper",
  "options": {
    "title": "filtrer",
    "open": true,
    "zoomOnFeature": true,
    "layers": [{
      "layerId": "boucle",
      "filter": [{
          "attribut": "secteur",
          "type": "combobox",
          "label": "Secteur",
          "updateOnChange": true
        },
        {
          "attribut": "classes_km",
          "type": "button",
          "label": "Distance",
          "updateOnChange": true
        },
        {
          "attribut": "classes_den_pos",
          "type": "button",
          "label": "Dénivelé",
          "updateOnChange": true
        }
      ]
    }]
  }
}
